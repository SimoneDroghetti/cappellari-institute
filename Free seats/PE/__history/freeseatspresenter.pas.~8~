unit freeseatspresenter;

interface

uses freeseatsinterface;

type
tcalcpresenter =class
  protected
  Fcalcview:Icalcview;
  public
  Constructor create(Acalcview: Icalcview);
  procedure eseguicontrollo;
end;

implementation


Constructor tcalcpresenter.create(Acalcview: Icalcview);
begin
  Fcalcview:=Acalcview;
end;

procedure tcalcpresenter.eseguicontrollo;
var Lgetval:integer;
begin
lgetval:=Fcalcview.Getvalue;
 // Fseatsview.DisplayMessage('eseguo controllo');

end;

end.
